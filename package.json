{
  "name": "calculadora_de_estrategias_de_derivativos",
  "version": "1.0.0",
  "description": "Refatoração em TypeScript para cálculo e otimização de estratégias com opções.",
  "type": "module",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "start": "node dist/src/index.js",
    "start-backend": "node dist/src/server.js",
    "dev": "tsx watch src/index.ts",
    "api:dev": "tsx watch src/server.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "process-csv": "tsx src/processador_opcoes.ts",
    "process-and-copy": "npm run process-csv && node scripts/copyCsv.js",
    "clean": "rimraf dist"
  },
  "keywords": [
    "opcoes",
    "derivativos",
    "boardpro",
    "finance"
  ],
  "author": "Marcio Souza",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.0",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.7",
    "express": "^4.19.0",
    "firebase": "^10.0.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.8.2",
    "mysql2": "^3.9.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.30",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5"
  }
}