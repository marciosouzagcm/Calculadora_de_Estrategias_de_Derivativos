{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "framework": "vite",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/analise",
      "destination": "/api/analise.ts"
    },
    {
      "source": "/api/buscar-opcoes",
      "destination": "/api/buscar-opcoes.ts"
    },
    {
      "source": "/api/cotacao",
      "destination": "/api/cotacao.ts"
    },
    {
      "source": "/api/salvar-estrategia",
      "destination": "/api/salvar-estrategia.ts"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}