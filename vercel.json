{
  "version": 2,
  "framework": "vite",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/saiba-mais/(.*)",
      "destination": "/saiba-mais/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}